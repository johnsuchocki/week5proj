package com.menuDB;

public class MenuItem {

	private static String menuID = null;
	private static String foodName = null;
	private static String cost = null;
	private static String timeServed = null;
	private static String healthyMenu = null;

	public MenuItem() {
		super();
	}

	public String getMenuID() {
		return menuID;
	}

	public void setMenuID(String menuID) {
		MenuItem.menuID = menuID;
	}

	public String getFoodName() {
		return foodName;
	}

	public void setFoodName(String foodName) {
		MenuItem.foodName = foodName;
	}

	public String getCost() {
		return cost;
	}

	public void setCost(String cost) {
		MenuItem.cost = cost;
	}

	public String getTimeServed() {
		return timeServed;
	}

	public void setTimeServed(String timeServed) {
		MenuItem.timeServed = timeServed;
	}

	public String getHealthyMenu() {
		return healthyMenu;
	}

	public void setHealthyMenu(String healthyMenu) {
		MenuItem.healthyMenu = healthyMenu;
	}

}
